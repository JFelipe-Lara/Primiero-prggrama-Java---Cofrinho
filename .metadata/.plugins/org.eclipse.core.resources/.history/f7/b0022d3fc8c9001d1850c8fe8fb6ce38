package Uninter;

public class Euro extends Moeda{
	
	public Euro(double valorIncial) {
		this.valor = valorIncial;  
	}
	
	public void info() {
		System.out.println("â‚¬ " + valor);
	}
	
	public double converter() {
		return this.valor * 5.65; //convertendo euro para real
	}
	
	public boolean equals(Object objeto) {
		if(this.getClass() != objeto.getClass()) {
			return false;
		}
		
		Euro objetoEuro = (Euro) objeto;
		
		if(this.valor != objetoEuro.valor) {
			return false;
		}
		return false;
	}

}
